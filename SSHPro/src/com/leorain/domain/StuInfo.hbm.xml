<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping schema="leorain" package="com.leorain.domain">
    	<class name="StuInfo" table="stuInfo">
    		<id column="stuId" name="stuId" type="java.lang.Integer">
    			<generator class="identity"></generator>
    		</id>
    		<property name="stuName" column="stuName" type="java.lang.String"></property>
    		<property name="stuCellPhone" column="stuCellPhone" type="java.lang.String"></property>
    		<property name="stuAddress" column="stuAddress" type="java.lang.String"></property>
    	</class>
    </hibernate-mapping>